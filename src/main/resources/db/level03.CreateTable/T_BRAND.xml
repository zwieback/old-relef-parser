<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog ../dbchangelog-3.5.xsd">

    <changeSet author="zwieback" id="RP-2: create table T_BRAND">
        <createTable tableName="T_BRAND">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false" primaryKeyName="pk_t_brand"/>
            </column>
            <column name="last_update" type="timestamp"/>
            <column name="name" type="varchar(255)"/>
            <column name="url" type="varchar(255)"/>
            <column name="image_url" type="varchar(255)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
